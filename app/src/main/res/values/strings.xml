<resources xmlns:xliff="http://schemas.android.com/apk/res-auto">
    <string name="app_name">Sunshine</string>
    <string name="action_settings">Settings</string>
    <string name="menu_refresh">Refresh</string>
    <string name="title_activity_detail">DetailActivity</string>
    <string name="title_settings_activity">SettingsActivity</string>
    <string name="preference_default_location_value">Kharkiv</string>
    <string name="preference_default_location_title">location</string>
    <string name="preference_default_location_key">location</string>
    <string name="preference_default_temp_key">Temperature Units</string>
    <string name="preference_default_temp_title">Temperature Units</string>
    <string name="preference_default_temp_summary">You need make a choice from units</string>
    <string name="preference_default_temp_default">metric</string>
    <string name="preference_default_temp_metric">metric</string>
    <string name="preference_default_temp_imperial">imperial</string>
    <string name="preference_map">See me on a map</string>
    <string name="action_share">Share</string>
    <string-array name="preference_list_temp_array">
        <item>Metric</item>
        <item>Imperial</item>
    </string-array>
    <string-array name="preference_list_temp_values">
        <item>@string/preference_default_temp_metric</item>
        <item>@string/preference_default_temp_imperial</item>
    </string-array>
    <!-- Date label when displaying today's weather forecast [CHAR LIMIT=20] -->
    <string name="today">Сегодня</string>

    <!-- Date label when displaying tomorrow's weather forecast [CHAR LIMIT=20] -->
    <string name="tomorrow">Завтра</string>

    <!-- Date format for displaying day of week and date (i.e. Mon Jun 1) [CHAR LIMIT=20] -->
    <string name="format_full_friendly_date">
        <xliff:g id="day">%1$s</xliff:g>,
        <xliff:g id="date">%2$s</xliff:g>
    </string>

    <!-- Strings for formatting weather-related data -->
    <!-- Temperature format [CHAR LIMIT=5] -->
    <string name="format_temperature"><xliff:g id="temp">%1.0f</xliff:g>\u00B0</string>

    <!-- Windspeed formats -->
    <!-- Wind in mph [CHAR LIMIT=25] -->
<string name="format_wind_mph">
    Wind: <xliff:g id="speed">%1$1.0f</xliff:g> mph <xliff:g id="direction">%2$s</xliff:g>
</string>

    <!-- Wind in kph [CHAR LIMIT=25] -->
<string name="format_wind_kmh">
    Wind: <xliff:g id="speed">%1$1.0f</xliff:g> km/h <xliff:g id="direction">%2$s</xliff:g>
</string>

    <!-- Pressure format CHAR LIMIT=25] -->
    <string name="format_pressure">Pressure: <xliff:g id="pressure">%1.0f</xliff:g> hPa</string>

    <!-- Humidity format CHAR LIMIT=25]-->
    <string name="format_humidity">Humidity: <xliff:g id="humidity">%1.0f</xliff:g> %%</string>

    
</resources>
